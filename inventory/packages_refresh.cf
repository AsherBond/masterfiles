bundle agent inventory_packages
# @brief Package inventory
#
# This bundle is for refreshing the package inventory.
{
  packages:
    debian::
      "cfe_internal_non_existing_package"
        package_policy => "add",
        package_method => dpkg_version("/");
    redhat::
      "cfe_internal_non_existing_package"
        package_policy => "add",
        package_method => rpm_version("/");
}
